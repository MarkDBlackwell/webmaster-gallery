<%= render :partial => 'sessions/review_group', :collection =>
        @review_groups -%>
<%= content_tag :div, :class => 'approve' do -%>
<%    if @approval_group -%>
<%=     render :partial => 'sessions/review_group', :object =>
            @approval_group -%>
<%=     form_for @approval_group, :url => {:controller => :sessions, :action =>
            :show}, :html => {:method => :get} do |f| -%><%= %>
<%=       submit_tag('approve') -%><%= %>
<%      end -%>
<%    end -%>
<%  end -%>
