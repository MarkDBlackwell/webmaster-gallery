<%# %%vi%%part%%app%%but -%>
<!--Session buttons-->
<%= content_tag 'div', :class => 'session-buttons' do %>
<%    unless @suppress_buttons -%>
<%=     button 'files', 'edit', :edit, :sessions -%><%= %>
<%=     button 'database problems', 'show', :show, :sessions -%><%= %>
<%=     button 'admin pictures', 'admin-pictures-index', :index,
            :admin_pictures -%><%= %>
<%=     content_tag 'div', :class => 'user-pictures-index' do -%>
<%=       content_tag 'form', :method => :get, :action => url_for(
              :controller => :pictures, :action => :index),
              :target => '_blank', :class => 'button_to' do -%>
<%=         content_tag 'div' do -%>
<%=           tag 'input', :type => 'submit', :value => 'user pictures' -%>
<%          end -%>
<%        end -%>
<%      end -%><%= %>
<%=     button 'log out', 'destroy', :destroy, :sessions, :delete -%>
<%    end -%>
<%  end -%><%= %>
